<html>
  <head>
    <%- include('./includes/head.ejs') %>

    <link href="api.css" rel="stylesheet" />
    <script src="api.js"></script>
  </head>
  <body>
    <h1 class="title">Uptrical API</h1>
    <div class="body">
      <% if (pages) { %> <% pages.forEach(element => { %>
      <div>
        <h5><%= element.title %></h5>
        <h6><%= element.description %></h6>
        <button id="/<%= element.href %>" onclick="showTerms()">
          Sayfaya Git
        </button>
      </div>
      <% }) %> <% } %>
    </div>
  </body>
  <footer>
    <div class="footer">
      <div class="row">
        <ul>
          <% if (footerLinks) { %> <% footerLinks.forEach(element => { %>
          <li><a href="<%= element.href %>"><%= element.title %></a></li>
          <% }) %> <% } %>
        </ul>
      </div>

      <div class="row">Copyright ©️ 2024 Uptrical Team</div>
    </div>
  </footer>
</html>
